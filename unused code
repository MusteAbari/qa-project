@app.route("/create")
def create():
    new_car = Car(reg= "P744AAY", 
    make="Mazda", 
    model="Xedos", 
    mileage=80000, 
    age=1995, 
    colour="Navy" 
    gearbox="Automatic" 
    price=500.00)

    db.session.add(new_car)
    db.session.commit()
    return "New Car Added"


class Customer(db.Model):
    customer_id = db.Column(db.Integer, primary_key=True)
    name = db.Column(db.String(50), nullable=False)
    number = db.Column(db.String(13), nullable=False)
    email = db.Column(db.String(50), nullable=False)

class Invoice(db.Model):
    invoivce_id = db.Column(db.Integer, primary_key=True)
    reg = db.Column(db.String(7), foreign_key=True)
    owner_id = db.Column(db.Integer, foreign_key=True)
    customer_id = db.Column(db.Integer, primary_key=True)
    date_bought = db.Column(db.DateTime, nullable=False, default=datetime.utcnow)


