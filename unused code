@app.route('/logout')
def logout():
    session.clear()
    return return render_template("index.html", title="Home", all_owners=all_owners)



@app.route("/addcar", methods=["GET", "POST"])
def addcar():
    form = RegisterForm()
    if request.method == "POST":
        new_owner = Owners(
            name=form.name.data,
            email=form.email.data,
            password=form.password.data,
            number=form.number.data)
        db.session.add(new_owner)
        db.session.commit()
        return redirect(url_for("home"))
    return render_template("register.html", title="Owner Registery", form=form)





